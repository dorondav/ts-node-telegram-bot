{
  "root": true,
  "env": {
    "node": true,
    "commonjs": true
},
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  //   "extends": [
  //     "eslint:recommended",
  //     "plugin:@typescript-eslint/eslint-recommended",
  //     "plugin:@typescript-eslint/recommended"
  //   ],
  "extends": ["plugin:prettier/recommended", "prettier", "eslint:recommended"],
  "rules": {
    "indent": [
        "error",
        "tab"
    ],
    "linebreak-style": [
        "error",
        "unix"
    ],
    "quotes": [
        "error",
        "single"
    ],
    "semi": [
        "error",
        "always"
    ]
},
"parserOptions": {
    "ecmaVersion": 2015
}
}
